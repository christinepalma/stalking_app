<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title>youtube</title>
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.5/css/bootstrap.min.css">
    <link rel="stylesheet" href="/stylesheets/youtube.css" media="screen" title="no title" charset="utf-8">
    <script src="http://code.jquery.com/jquery-1.11.3.min.js"></script>
    <script src="/javascripts/googleplus.js"></script>
  </head>
  <body>
    <div class="panel panel-default">
      <div class="panel-heading"><a id="cTitle" href="http://www.youtube.com/channel/<%=data.items[0].snippet.channelId%>" target="_blank"><%=(data.items[0].snippet.channelTitle)%></p></div>
    </div>
    <% data.items.forEach(function(item){ %>
      <div class="panel panel-default">
        <div class="panel-body">
          <a id="first" href="http://www.youtube.com/watch?v=<%=item.id.videoId%>" target="_blank"><img class="tn" src="<%=item.snippet.thumbnails.default.url%>" alt=""/></a>
          <a id="title" href="http://www.youtube.com/watch?v=<%=item.id.videoId%>" target="_blank"> <%=(item.snippet.title)%></a>
          <p id="des">Description:</p><p><%=(item.snippet.description)%></p>
          <p class="feed_time" data-time="<%=item.snippet.publishedAt%>"></p>
        </div>
      </div>
    <% }) %>
  </body>
</html>
