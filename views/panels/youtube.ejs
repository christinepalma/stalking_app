<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title>youtube</title>
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.5/css/bootstrap.min.css">
    <link rel="stylesheet" href="/stylesheets/youtube.css" media="screen" title="no title" charset="utf-8">
  </head>
  <body>
    <div class="youtube">

      <% data.items.forEach(function(item){ %>

          <div class="row">
            <div class="col-md-4"><a href="http://www.youtube.com/watch?v=<%=item.id.videoId%>" target="_blank"><img class="tn" src="<%=item.snippet.thumbnails.default.url%>" alt=""/></a></div>
            <div class="col-md-8">
              <a id="title" href="http://www.youtube.com/watch?v=<%=item.id.videoId%>" target="_blank"> <%=(item.snippet.title)%></a>  <br>
              <p id="des">Description:</p><p><%=(item.snippet.description)%></p><br>
              <a id="cTitle" href="http://www.youtube.com/channel/<%=item.snippet.channelId%>" target="_blank">By: <%=(item.snippet.channelTitle)%></p>
            </div>
          </div>


      <% }) %>

              </div>








  </body>
</html>
